# Exclude VS Code configuration folders
.vscode/
.vscode-test/
.idea/

# Exclude test directories
test/

# Exclude source files and TypeScript-specific files since we only ship the compiled output
src/
**/*.ts
**/*.tsx

# Exclude config files not needed at runtime (adjust as needed)
tsconfig.json

# Ensure the compiled output directory is included in the package
!out/**

# Other files
.github/
.gitignore
.git/

# Package Manager Files (Usually not needed, can sometimes cause issues)
package-lock.json
eslint.config.mjs

# Exclude config files for IDE extensions
.eslintrc.*
.prettierrc.*
.DS_Store